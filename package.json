{
  "name": "playlister",
  "version": "0.1.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "lessc less/main.less public/css/style.css",
    "build:js": "browserify -t [ babelify --presets [ react ] ] src/main.js -o public/js/app.js --debug",
    "watch:css": "watch \"npm run build:css\" less",
    "watch:js": "watch \"npm run build:js\" src",
    "livereload": "live-reload --port 3001 public/",
    "serve": "http-server -p 4001",
    "dev": "parallelshell \"npm run serve\" \"npm run livereload\" \"npm run watch:js\" \"npm run watch:css\""
  },
  "repository": {},
  "author": "Matita",
  "license": "ISC",
  "dependencies": {
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-request": "^0.3.3",
    "musicbrainz": "^0.2.6",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-player": "^0.8.0",
    "shuffle-array": "^1.0.0",
    "youtube-search": "^1.0.6"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "less": "^2.6.1",
    "less-plugin-autoprefix": "^1.5.1",
    "live-reload": "^1.1.0",
    "parallelshell": "^2.0.0",
    "watch": "^0.17.1"
  }
}
